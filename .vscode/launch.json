{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Cortex Debug (ST-Link, Generic)",
      "type": "cortex-debug",
      "request": "launch",
      "servertype": "openocd",
      "cwd": "${workspaceFolder}",
      "executable": "${workspaceFolder}/${command:makefile.getConfigValue}",
      "device": "STM32F407VG",
      "configFiles": [
        "interface/stlink.cfg",
        "target/stm32f4x.cfg"
      ],
      "runToEntryPoint": "main",
      "preLaunchTask": "build",
      "openOCDLaunchCommands": [
        "init",
        "reset init"
      ]
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "e:/dev/STM32/STM32F4-DISCO-Projects/Drivers/UART/src",
      "program": "e:/dev/STM32/STM32F4-DISCO-Projects/Drivers/UART/src/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}